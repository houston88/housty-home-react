{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "deploynotest": "better-npm-run deploynotest",
    "deploynotest:prod": "better-npm-run deploynotest:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/dev-server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploynotest": {
      "command": "npm run lint && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "deploynotest:prod": {
      "command": "npm run deploynotest",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/dev-server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start config/karma.config",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "babel-core": "~6.26.0",
    "babel-loader": "~6.4.1",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-es2015": "~6.24.1",
    "babel-preset-react": "~6.24.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-runtime": "~6.26.0",
    "better-npm-run": "0.0.13",
    "compression": "~1.7.1",
    "css-loader": "~0.26.4",
    "cssnano": "^3.7.4",
    "datamaps": "^0.5.8",
    "debug": "~2.6.9",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "~2.30.1",
    "imports-loader": "~0.7.1",
    "ip": "~1.1.5",
    "json-loader": "~0.5.7",
    "node-sass": "^7.0.1",
    "normalize.css": "^5.0.0",
    "postcss-loader": "~1.3.3",
    "react": "~15.6.2",
    "react-addons-css-transition-group": "~15.6.2",
    "react-dom": "~15.6.2",
    "react-fontawesome": "~1.6.1",
    "react-parallax": "~1.5.1",
    "react-redux": "~5.0.6",
    "react-router": "~3.2.0",
    "redux": "~3.7.2",
    "redux-thunk": "^2.0.0",
    "rimraf": "~2.6.2",
    "sass-loader": "^4.0.0",
    "style-loader": "~0.13.2",
    "url-loader": "~0.5.9",
    "webpack": "~1.15.0",
    "whatwg-fetch": "^3.0.0",
    "yargs": "^6.3.0"
  },
  "devDependencies": {
    "babel-eslint": "~7.2.3",
    "babel-plugin-istanbul": "^3.0.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^1.0.1",
    "enzyme": "~2.9.1",
    "eslint": "~3.19.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "~4.3.0",
    "eslint-plugin-babel": "~4.1.2",
    "eslint-plugin-promise": "~3.6.0",
    "eslint-plugin-react": "~6.10.3",
    "eslint-plugin-standard": "~2.3.1",
    "express": "~4.17.3",
    "karma": "~1.7.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "~2.2.5",
    "karma-phantomjs-launcher": "~1.0.4",
    "karma-webpack-with-fast-source-maps": "~1.10.2",
    "mocha": "~3.5.3",
    "nodemon": "^1.19.2",
    "phantomjs-prebuilt": "~2.1.16",
    "react-addons-test-utils": "~15.6.2",
    "redbox-react": "~1.5.0",
    "sinon": "^1.17.5",
    "sinon-chai": "~2.14.0",
    "webpack-dev-middleware": "~1.12.2",
    "webpack-hot-middleware": "~2.21.0"
  }
}
